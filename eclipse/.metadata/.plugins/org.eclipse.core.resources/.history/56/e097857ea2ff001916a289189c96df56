package com.ustglobal.maps;

import java.util.Hashtable;

public class TestH {

	public static void main(String[] args) {
		
		Hashtable<Integer, String> ht = new Hashtable<Integer, String>();
		ht.put(101, "ajay");
		ht.put(102, "vijay");
		ht.put(103, "arjun");
		ht.put(104, "kajay");
		ht.put(105, "majay");
		ht.put(101, "shriram");
//		ht.put(null, "john"); we cant had not even one null value
		ht.put(108,null);
		
		
		System.out.println(" data are : "+ht);
	}
}
